{
    "name": "rayafort/plans",
    "description": "SaaS style Plans and Features for Laravel.",
    "keywords": ["plans", "laravel", "memberships"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "nesbot/carbon": "~2.17",
        "illuminate/support": "~5.8",
        "illuminate/database": "~5.8"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.8",
        "orchestra/testbench": "~3.3.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "RayaFort\\Plans\\": "src/RayaFort/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "RayaFort\\Plans\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
