{
    "name": "rayafort/plans",
    "description": "SaaS style Plans and Features for Laravel.",
    "keywords": ["plans", "laravel", "memberships"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "illuminate/database": "~5.8",
        "illuminate/support": "~5.8",
        "nesbot/carbon": "~2.17"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.8",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.4.0"
    },
    "autoload": {
        "psr-4": {
            "RayaFort\\Plans\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RayaFort\\Plans\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RayaFort\\Plans\\PlansServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
